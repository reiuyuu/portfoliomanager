{
  "name": "portfoliomanager",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run web:dev\" \"npm run server:dev\"",
    "build": "concurrently \"npm run build:web\" \"npm run build:server\"",
    "install:all": "npm install && cd web && npm install && cd ../server && npm install",
    "web:dev": "cd web && npm run dev",
    "web:build": "cd web && npm run build",
    "server:dev": "cd server && npm run dev",
    "server:build": "cd server && npm run build",
    "format": "prettier --write .",
    "clean": "concurrently \"cd web && npm run clean\" \"cd server && npm run clean\""
  },
  "keywords": [],
  "author": "reiuyuu <reiuyuu@gmail.com>",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "concurrently": "^9.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
